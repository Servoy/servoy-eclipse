 </extension>
 <extension
     point = "org.eclipse.ui.propertyPages">
        <page
              class="com.servoy.eclipse.ui.views.SolutionPropertiesPage"
              id="com.servoy.eclipse.ui.views.SolutionPropertiesPage"
              name="Solution Properties"
              objectClass="com.servoy.eclipse.ui.views.solutionexplorer.PlatformSimpleUserNode">
        </page>
         <page
               class="com.servoy.eclipse.ui.preferences.ServoyErrorWarningPreferencePage"
               id="com.servoy.eclipse.ui.propertyPage.error.warning"
               name="Servoy Errors/Warnings">
            <enabledWhen>
               <adapt
                     type="org.eclipse.core.resources.IProject">
                  <test
                        property="org.eclipse.core.resources.projectNature"
                        value="org.eclipse.dltk.javascript.core.nature">
                  </test>
               </adapt>
            </enabledWhen>
        </page>
		<page
			category="org.eclipse.dltk.javascript.editor.propertyPage"
			name="Externalize Strings"
			class="com.servoy.eclipse.ui.preferences.JSFileExternalizeNLSRemoverConfigurationPage"
			id="org.eclipse.dltk.javascript.editor.propertyPage.removeNLS">
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject">
					<test property="org.eclipse.core.resources.projectNature" value="org.eclipse.dltk.javascript.core.nature"/>
				</adapt>
			</enabledWhen>
		</page>
  </extension>
