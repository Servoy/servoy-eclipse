<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Servoy"
            id="com.servoy.eclipse.ui">
      </category>
      <view
            name="Solution Explorer"
            icon="icons/solution.gif"
            category="com.servoy.eclipse.ui"
            class="com.servoy.eclipse.ui.views.solutionexplorer.SolutionExplorerView"
            id="com.servoy.eclipse.ui.views.SolutionExplorerView">
      </view>      
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.TaskList"
               relationship="right"
               id="com.servoy.eclipse.ui.views.SolutionExplorerView">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.team.ui.TeamSynchronizingPerspective">
         <view
               id="org.eclipse.ui.navigator.ProjectExplorer"
               relationship="stack"
               relative="org.eclipse.team.sync.views.SynchronizeView">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="com.servoy.eclipse.ui.DesignPerspective">
         <viewShortcut
               id="com.servoy.eclipse.ui.views.SolutionExplorerView">
         </viewShortcut>
		 <showInPart
               id="com.servoy.eclipse.ui.views.SolutionExplorerView">
         </showInPart>
         <showInPart
               id="org.eclipse.ui.views.ResourceNavigator">
         </showInPart>
         <actionSet
               id="com.servoy.eclipse.ui.servoySearchActionSet">
         </actionSet>
         <actionSet
               id="org.eclipse.debug.ui.breakpointActionSet">
         </actionSet>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         <view
               id="org.eclipse.debug.ui.ExpressionView"
               ratio="0.5"
               relationship="stack"
               relative="org.eclipse.debug.ui.VariableView"
               visible="true">
         </view>
         <actionSet
               id="com.servoy.eclipse.ui.servoySearchActionSet">
         </actionSet>
         <perspectiveShortcut
               id="com.servoy.eclipse.ui.DesignPerspective">
         </perspectiveShortcut>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.servoy.eclipse.ui.DesignPerspective"
            icon="icons/windowicon.gif"
            id="com.servoy.eclipse.ui.DesignPerspective"
            name="Servoy Design">
      </perspective>
   </extension>
