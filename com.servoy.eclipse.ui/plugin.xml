      </perspectiveExtension>
      <perspectiveExtension
            targetID="com.servoy.eclipse.ui.DesignPerspective">
         <viewShortcut
               id="com.servoy.eclipse.ui.views.SolutionExplorerView">
         </viewShortcut>
		 <showInPart
