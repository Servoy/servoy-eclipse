   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <category
            id="com.servoy.eclipse.ui.CheatSheetCategory"
            name="Servoy">
      </category>
      <cheatsheet
            category="com.servoy.eclipse.ui.CheatSheetCategory"
            composite="false"
            contentFile="cheatsheets/CreateSolution.xml"
            id="com.servoy.eclipse.ui.cheatsheet.createsolution"
            name="Create a Servoy Solution">
         <description>
			Shows you how to create a Servoy solution and how to run it with the Smart Client and the Web Client.
         </description>
      </cheatsheet>
      <cheatsheet
            category="com.servoy.eclipse.ui.CheatSheetCategory"
            composite="false"
            contentFile="cheatsheets/CreateDataModel.xml"
            id="com.servoy.eclipse.ui.cheatsheet.createdatamodel"
            name="Create Data Model">
         <description>
			Shows you how to create tables for your data model.
         </description>
      </cheatsheet>
      <cheatsheet
            category="com.servoy.eclipse.ui.CheatSheetCategory"
            composite="false"
            contentFile="cheatsheets/CreateForms.xml"
            id="com.servoy.eclipse.ui.cheatsheet.createui"
            name="Create Graphical User Interface">
         <description>
			Shows you how to create forms for your graphical user interface.
         </description>
      </cheatsheet>
