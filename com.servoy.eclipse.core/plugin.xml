   <extension
         id="com.servoy.eclipse.core.servoyBuilder"
         name="Servoy Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            callOnEmptyDelta="true"
            hasNature="false"
            isConfigurable="true">
