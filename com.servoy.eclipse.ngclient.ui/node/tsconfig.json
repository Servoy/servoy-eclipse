{
  "compileOnSave": true,
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "module": "esnext",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
     "core-js/es7/reflect": ["../node_modules/core-js/proposals/reflect-metadata"]
    },
		"outDir": "./out-tsc/app"
  },
	"buildOnSave": true
}
