{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "module": "es2020",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "lib": [
      "es2017",
      "dom",
      "ES2020.Promise",
       "dom.Iterable"
    ],
    "paths": {
      "@servoy/public": [
        "../projects/servoy-public/src/public-api",
        "./projects/servoy-public/src/public-api",
        "../servoy-public/src/public-api"
      ]
    },
	"outDir": "./out-tsc/app"
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
