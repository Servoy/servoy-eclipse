  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --subresource-integrity=true ngclient2",
    "build_sourcemap": "ng build ngclient2 --configuration production --subresource-integrity=true --source-map",
    "build_lib": "ng build @servoy/public --configuration production",
    "build_lib_debug_nowatch": "ng build @servoy/public --watch=false",
    "build_debug": "ng build ngclient2 --watch",
    "build_debug_nowatch": "ng build ngclient2 --watch=false",
    "build_lib_debug": "ng build @servoy/public --watch",

    "test_public": "ng test @servoy/public --watch --browsers Chrome",
    "test_default": "ng test @servoy/servoydefault --watch --browsers Chrome",
    "test_dialogs": "ng test @servoy/dialogs --watch --browsers Chrome",
    "test_edge": "ng test --watch --browsers Edge",
    "test_edge_nowatch": "ng test --watch=false --browsers Edge",
    "test_default_edge_nowatch": "ng test @servoy/servoydefault --watch=false --browsers Edge",
    "test_public_edge": "ng test @servoy/public --watch --browsers Edge",
    "test_public_edge_nowatch": "ng test @servoy/public --watch=false --browsers Edge",

    "test_headless": "ng test --browsers headlessChrome --watch=false",
    "test_dev_all_nowatch": "ng test --watch=false --karma-config karma.dev.once.conf.js",

    "test_dev_only_src_dir": "ng test ngclient2 --watch --karma-config karma.dev.conf.js",
    "test_dev_dialogs": "ng test @servoy/dialogs --watch --karma-config karma.dev.conf.js",
    "test_dev_ngclientutils": "ng test @servoy/ngclientutils --watch --karma-config karma.dev.conf.js",
    "test_dev_public": "ng test @servoy/public --watch --karma-config karma.dev.conf.js",
    "test_dev_default": "ng test @servoy/servoydefault --watch --karma-config karma.dev.conf.js",
    "test_dev_window": "ng test @servoy/window --watch --karma-config karma.dev.conf.js",
    
    "test_dev_only_src_dir_chrome": "ng test ngclient2 --watch --browsers Chrome --karma-config karma.dev.conf.js",
    "test_dev_dialogs_chrome": "ng test @servoy/dialogs --watch --browsers Chrome --karma-config karma.dev.conf.js",
    "test_dev_ngclientutils_chrome": "ng test @servoy/ngclientutils --watch --browsers Chrome --karma-config karma.dev.conf.js",
    "test_dev_public_chrome": "ng test @servoy/public --watch --browsers Chrome --karma-config karma.dev.conf.js",
    "test_dev_default_chrome": "ng test @servoy/servoydefault --watch --browsers Chrome --karma-config karma.dev.conf.js",
    "test_dev_window_chrome": "ng test @servoy/window --watch --browsers Chrome --karma-config karma.dev.conf.js",

    "update": "ng update"
  },
